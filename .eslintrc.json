{
   "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
  "plugins": ["prettier"],
      "parserOptions": {
      "parser": "babel-eslint"
    },
  "rules": {
    "prettier/prettier": "warn"
  }

}

